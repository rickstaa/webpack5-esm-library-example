    "clean": "rimraf node_modules .pnpm-debug.log package-lock.json dist",
    "stats": "webpack --config webpack.prod.js --json build-stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "babel-loader": "^8.2.3",
    "rimraf": "^3.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "type": "module",
  "optionalDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
