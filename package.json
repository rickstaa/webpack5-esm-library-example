{
  "name": "webpack5-library-example",
  "version": "1.0.0",
  "description": "A webpack5 esm library example package.",
  "exports": "./dist/webpack5-esm-library-example.bundle.mjs",
  "browser": "./dist/webpack5-esm-library-example.bundle.mjs",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:watch": "webpack --config webpack.dev.js -w",
